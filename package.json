{
  "name": "graduation-invitation-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/print-network-address.js && next dev --hostname 0.0.0.0 --port 3002",
    "build": "next build",
    "build:export": "next build && next export",
    "deploy": "next build && next export && rm -rf docs/* && mkdir -p docs && cp -R out/* docs/ && touch docs/.nojekyll",
    "art:png": "node scripts/generate-artwork.js",
    "start": "next start --hostname 0.0.0.0 --port 3002",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "sharp": "^0.34.4",
    "typescript": "^5.4.5"
  }
}
